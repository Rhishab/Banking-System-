        int length = sprintf(FirstName, "%s", FirstName);
        int NameArr(length);
        int i;
        for (int i = 0; i < length; i++) {
            NameArr(i) = FirstName.at(i) << endl; //get character at position i
        }
        for (int i = 0; i < length; i++) {
            NameArr(i);
        }




