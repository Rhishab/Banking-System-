#include <iostream>
#include<string>
using namespace std;

class ArrayData {
private : 
    int  arr[3][2][3];  // [Main Array num][Sub Array in each Main][element storage ]
public:
    int i, j, k, l;

    ArrayData() {
        for (j = 0;j < 3;j++) {
            for (k = 0;k < 3;k++) {
                for (l = 0;l < 3;l++) {
                    cout << "Enter number  " << endl;
                    cin >> arr[j][k][l];

                }
                cout << "Next column" << endl;
            }
            cout << "Net Row" << endl;

        }

        
    }

    void display() {
        for (a = 0;a < 3;a++) {
            for (b = 0;b < 3;b++) {
                cout << "[";
                for (c = 0;c < 3;c++) {
                    cout << arr[a][b][c] << " ";

                }
                cout << "]" << endl;
            }
            cout << endl;
        }

    }



};

int main() {
    ArrayData A;
    A.display();

    //int arr[3][2][3];// [Main Array num][Sub Array in each Main][element storage ]


    int i, j, k, l;


    
    cout << "'Your file has been inserted" << endl;
    cout << "What options would you like to do" << endl;

    // Orginal :
    int a, b, c;
    
    int g;

    cout << " Hello ";
    cin >> g;







    return 0;
}
