#include <string>
#include <iostream>
using namespace std;

class Data {
private:
    int Arr[3][2][2];

public:

    Data() {
        int j, l, k;
        int z = 1;
        for (j = 0;j <= 3;j++) {
            for (k = 0;k <= 2;k++) {
                for (l = 0;l <= 2;l++) {
                    Arr[j][k][l] = z;
                    z = z + 1;
                }
            }
        } // end of declaration 
    }

    void display() {
        int a, b, c;
        for (a = 0;a < 3;a++) {
            for (b = 0;b <= 2;b++) {
                cout << "[";
                for (c = 0;c <= 2;c++) {
                    cout << Arr[a][b][c] << " ";

                }
                cout << "]" << endl;
            }
            cout << endl;
        }

    } // end of display

};

int main()
{ 
    Data D1;
    D1.display();

    return 0;

    
}
